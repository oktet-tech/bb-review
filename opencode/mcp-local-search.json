{
  "$schema": "https://opencode.ai/config.json",
  "model": "openrouter/google/gemini-3-pro-preview",
  "permission": {
    "edit": "deny",
    "bash": "deny"
  },
  "mcp": {
    "te-dev": {
      "type": "local",
      "command": ["bb-review", "cocoindex", "serve", "te-dev"],
      "environment": {
        "COCOINDEX_DATABASE_URL": "postgresql://cocoindex:cocoindex@localhost:5432/cocoindex"
      },
      "enabled": true
    }
  }
}
